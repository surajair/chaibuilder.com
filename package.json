{
  "name": "chaibuilder-nextjs",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npx chai-tailwind --watch\" \"next dev --port 3001 --turbopack\"",
    "build": "npx chai-tailwind && next build",
    "dev-no-check": "pnpm install && next dev --turbopack",
    "build-no-check": "pnpm install && pnpm run prebuild && next build --turbopack",
    "start": "next start",
    "lint": "next lint & tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-env": "node scripts/check-env.cjs"
  },
  "dependencies": {
    "@next/third-parties": "^15.4.6",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.5",
    "@supabase/supabase-js": "2.50.3",
    "chai-next": "0.0.15-beta",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.539.0",
    "marked": "^16.1.2",
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "shiki": "^3.9.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^8",
    "eslint-config-next": "^15.4",
    "fs-extra": "^11.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.8",
      "@types/react-dom": "19.0.3"
    }
  }
}
