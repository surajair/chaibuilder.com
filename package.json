{
  "name": "chaibuilder-nextjs",
  "version": "0.1.4",
  "private": true,
  "scripts": {
    "dev": "pnpm install && node scripts/check-env.cjs && concurrently \"node scripts/tailwind.cjs --dev\" \"next dev --turbopack\"",
    "prebuild": "node scripts/tailwind.cjs",
    "build": "next build",
    "dev-no-check": "pnpm install && next dev",
    "build-no-check": "pnpm install && pnpm run prebuild && next build",
    "start": "next start",
    "lint": "next lint & tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "check-env": "node scripts/check-env.cjs"
  },
  "dependencies": {
    "@chaibuilder/pages": "~0.7.2",
    "@chaibuilder/sdk": "~2.3.2",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.62.8",
    "axios": "^1.7.8",
    "canvas": "^3.1.0",
    "cmdk": "1.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "0.471.1",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@mhsdesign/jit-browser-tailwindcss": "^0.4.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.17.12",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.3.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-eslint": "^16.3.0",
    "prettier-eslint-cli": "^8.0.1",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.8",
      "@types/react-dom": "19.0.3"
    }
  }
}
